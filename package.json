{
  "name": "minanft-api",
  "description": "Mina NFT backend, API and telegram bot",
  "keywords": [
    "minanft",
    "NFT",
    "backend",
    "bot",
    "telegram",
    "mina",
    "zkapp",
    "zk",
    "blockchain",
    "web3",
    "zero knowledge"
  ],
  "version": "1.6.0",
  "author": "dfst.io",
  "license": "Apache-2.0",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "deploy": "serverless deploy --aws-profile minanft",
    "compile": "serverless package --aws-profile minanft",
    "info": "serverless info --aws-profile minanft",
    "org": "serverless --org=dfstio --aws-profile minanft",
    "push": "git status && git push -u originminanft contract",
    "gitcreate": "git remote add originminanftapi https://github.com/dfstio/minanft-api",
    "githubcleartoken": "git config --global --unset credential.helper",
    "checkout": "git checkout -b contract",
    "commit": "git commit -a -S -m",
    "lint": "eslint 'src/*.{ts,tsx}' handler.ts",
    "style": "prettier --write {,!(node_modules)/**/}*.ts",
    "upgradeinteractive": "yarn upgrade-interactive",
    "ipfs": "ts-node test/ipfs3.ts",
    "faucet": "ts-node test/faucet.ts",
    "account": "ts-node test/account.ts",
    "verify": "ts-node test/verify.ts",
    "lang": "ts-node test/lang.ts",
    "deploytest": "ts-node test/deploy.ts",
    "deployers": "ts-node test/deployers.ts",
    "generate": "ts-node test/generate.ts",
    "vscode": "yarn dlx @yarnpkg/sdks vscode"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.13",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "serverless-plugin-typescript": "^2.1.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.441.0",
    "@aws-sdk/client-elastic-transcoder": "^3.441.0",
    "@aws-sdk/client-lambda": "^3.441.0",
    "@aws-sdk/client-s3": "^3.441.0",
    "@aws-sdk/util-dynamodb": "^3.441.0",
    "@types/aws-lambda": "^8.10.126",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/remove-markdown": "^0.3.3",
    "algoliasearch": "^4.20.0",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "i18next": "^23.6.0",
    "jsonwebtoken": "^9.0.2",
    "o1js": "^0.14.0",
    "openai": "^4.16.1",
    "remove-markdown": "^0.5.0",
    "telegraf": "^4.15.0"
  },
  "packageManager": "yarn@4.0.1"
