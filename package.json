{
  "name": "minanft-api",
  "description": "Mina NFT backend, API and telegram bot",
  "keywords": [
    "minanft",
    "NFT",
    "backend",
    "bot",
    "telegram",
    "mina",
    "zkapp",
    "zk",
    "blockchain",
