    "serverless-prune-plugin": "^2.0.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.606.0",
    "@aws-sdk/client-elastic-transcoder": "^3.606.0",
