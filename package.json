    "zero knowledge"
  ],
  "version": "1.6.0",
  "author": "dfst.io",
  "license": "Apache-2.0",
  "scripts": {
    "deploy": "rm -rf .build && serverless deploy --aws-profile minanft  --use-local-credentials",
    "prune": "serverless prune --aws-profile minanft -n 5",
    "build": "rm -rf .build && serverless package --aws-profile minanft",
    "info": "serverless info --aws-profile minanft",
    "org": "serverless --org=dfstio --aws-profile minanft"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.140",
